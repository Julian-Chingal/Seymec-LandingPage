---
import Banner from "@/components/landing/Banner.astro";
import { services } from "@/data/servicesData";

const data = services.find((service) => service.title === "Servicios Electromecánicos e Industriales");
---
  {data ? (
    <Banner
      title="Electromecánicos e Industriales"
      img={data.img}
    >
      <h1>aqui los servicios de electronica e instrumentacion</h1>
    </Banner>
  ) : (
    <p>Service not found</p>
  )}

